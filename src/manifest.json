{
  "manifest_version": 2,

  "name": "Maven Central Search",
  "description": "Adds an “mvn” omnibox command for the Maven Central.",
  "version": "0.2.0",
  "icons": {
    "512": "icons/512.png"
  },

  "permissions": [
    "tabs",
    "https://search.maven.org/*"
  ],
  "omnibox": {
    "keyword": "mvn"
  },
  "background": {
    "scripts": [
      "omnibox.js"
    ]
  }
}
